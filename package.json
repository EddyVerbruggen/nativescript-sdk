{
  "name": "kinvey-nativescript-sdk",
  "version": "3.4.5",
  "description": "Kinvey JavaScript SDK for NativeScript applications.",
  "homepage": "http://www.kinvey.com",
  "bugs": {
    "url": "https://github.com/Kinvey/nativescript-sdk/issues"
  },
  "license": "Apache-2.0",
  "author": "Kinvey",
  "contributors": [
    "Thomas Conner <thomas@kinvey.com>"
  ],
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:Kinvey/nativescript-sdk"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf coverage",
    "cover": "istanbul cover _mocha -- --compilers js:babel-core/register -r babel-polyfill -s 100 --recursive test/unit/index test/unit",
    "lint": "npm run lint:src",
    "lint:src": "eslint src/**",
    "lint:test": "eslint test/unit/**",
    "preversion": "rm -rf dist && npm install && npm test",
    "postversion": "git push && git push --tags && rm -rf coverage",
    "test": "mocha --compilers js:babel-core/register -r babel-polyfill -s 100 --recursive test/unit/index test/unit",
    "test:watch": "mocha -w --compilers js:babel-core/register -r babel-polyfill -s 100 --recursive test/unit/index test/unit"
  },
  "dependencies": {
    "events": "1.1.1",
    "kinvey-js-sdk": "3.5.0"
  },
  "devDependencies": {
    "@types/node": "7.0.22",
    "eslint": "3.19.0",
    "eslint-config-airbnb-base": "11.1.3",
    "eslint-import-resolver-babel-module": "3.0.0",
    "eslint-plugin-import": "2.2.0",
    "nativescript-dev-typescript": "0.4.5",
    "tns-core-modules": "3.0.1",
    "tns-platform-declarations": "3.0.1",
    "typescript": "2.3.3"
  },
  "nativescript": {
    "platforms": {
      "ios": "3.0.0",
      "android": "3.0.0"
    }
  },
  "keywords": [
    "Kinvey",
    "JavaScript",
    "NativeScript"
  ]
}
